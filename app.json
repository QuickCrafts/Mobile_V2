{
  "expo": {
    "name": "QuickFins",
    "slug": "QuickFins",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/full_icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/QuickFins.png",
      "resizeMode": "contain",
      "backgroundColor": "#0C0911"
    },
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/App_icon_v1.png"
    },
    "plugins": [
      "expo-router",
      [ 
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/baloo-bhai-2/BalooBhai2_400Regular.ttf",
            "node_modules/@expo-google-fonts/baloo-bhai-2/BalooBhai2_500Medium.ttf",
            "node_modules/@expo-google-fonts/baloo-bhai-2/BalooBhai2_600SemiBold.ttf",
            "node_modules/@expo-google-fonts/baloo-bhai-2/BalooBhai2_700Bold.ttf",
            "node_modules/@expo-google-fonts/baloo-bhai-2/BalooBhai2_800ExtraBold.ttf",
            "android/app/src/main/assets/BalooBhai2_400Regular.ttf",
            "android/app/src/main/assets/BalooBhai2_500Medium.ttf",
            "android/app/src/main/assets/BalooBhai2_600SemiBold.ttf",
            "android/app/src/main/assets/BalooBhai2_700Bold.ttf",
            "android/app/src/main/assets/BalooBhai2_800ExtraBold.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
